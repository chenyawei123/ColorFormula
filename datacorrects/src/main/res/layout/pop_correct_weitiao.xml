<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:background="@drawable/bg_white_white_corner"
    android:paddingBottom="@dimen/private_center_padding">

    <include
        layout="@layout/correct_pop_title"
        ></include>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/tb"
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:layout_marginTop="10dp"
        android:focusable="false"></androidx.recyclerview.widget.RecyclerView>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">

        <Button
            android:id="@+id/btn_cancel_wt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_gray_gray_corner"
            android:text="@string/cancel"
            android:textColor="@color/white"
            android:textSize="@dimen/typeface_size"/>

        <Button
            android:id="@+id/btn_save_wt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/private_center_padding_big"
            android:background="@drawable/tv_blue_orange_selector"
            android:text="保存"
            android:textColor="@color/white"
            android:textSize="@dimen/typeface_size"/>
    </LinearLayout>


</LinearLayout>
